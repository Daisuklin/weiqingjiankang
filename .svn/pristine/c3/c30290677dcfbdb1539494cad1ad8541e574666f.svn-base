/*登录界面*/
.imgbackrground{
  text-align: center;padding: 1.7rem 0px 0.12rem;
  .text_p{font-size: 0.28rem;color:#333333;}
}
.geneLoginlist{
  .am-list-item.am-input-item {
    height: 0.9rem;
    padding-left: 32px !important;
    padding-right: 32px !important;
    margin: 0.1rem 0.5rem;
    font-size: 0.26rem;
    color:#999999;
  }
  .am-list-item.am-input-item input{
    font-size: 0.26rem;
    color:#999999;
  }
  .am-list-item .am-input-label.am-input-label-5{width:auto}
  .queryButton{
    background: none;
    padding: 30vh 0rem 0rem;
    margin: 0px 0rem;
    width: 100%;

    .am-button-primary{background: #5491d2;font-size: 0.28rem;height: 0.7rem;line-height: 0.7rem;margin: 0px 0.5rem;}
  }
  @media screen and (device-aspect-ratio: 40/71) {
    .queryButton{
      padding: 18vh 0rem 0rem;
    }
  }
.nameButtom{
  .am-input-extra{max-height: 0.6rem;}
}
  .am-list-footer{padding-left: 0.5rem;}
}
