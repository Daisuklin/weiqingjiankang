.ViewLogistics-scroll{
  .viewLogistice_list{
    .am-list-item-middle{
      border-bottom: 1px solid #e5e5e5;
      padding: 0px 0.26rem;
      height:0.9rem;
      .am-list-line:after{border-bottom: none;}
      .am-list-content{font-size: 0.28rem;color:#333;}
      .am-list-extra{font-size: 0.24rem;color:#999;}
    }
  }
  //流程
  .viewLogistice_process{
    .list-progress{
      li {
        position: relative;
        list-style: none;
      }
      //li:first-child:before{ background:url("../../../assets/img/weiqing/wuliu-01@2x.png") no-repeat;}
      li:before {
        position: absolute;
        left: -.6rem;
        top: 0;
        content: "";
        width: .66666667rem;
        height: .64rem;
        background:url("../../../assets/img/weiqing/wuliu-01@2x.png") no-repeat;
        background-size: 40% 40%;
      }
      li .time {
        color: #999;
        font-size: 0.24rem;
        margin-bottom: .13333333rem;
        font-weight: 400;

      }
      li p {
        color: #666;
        font-size: 0.26rem;
        line-height: 0.35rem;
        margin-bottom: .33333333rem;
        word-break: break-all;
      }
      li:after {
        position: absolute;
        left: -0.48rem;
        top: .29rem;
        bottom: -0.35rem;
        content: "";
        width: .01333333rem;
        border-left: 1px solid #999;
      }
      li .single-thumbs img {
        float: left;
        width: 2rem;
        height: 2rem;
        margin-right: .2rem;
        margin-bottom: .2rem;
      }

    }
  }
}